    <Style x:Key="Pump" TargetType="{x:Type Shape}">

        <Setter Property="StrokeThickness" Value="3" />
        <Setter Property="StrokeMiterLimit" Value="1" />
        <Setter Property="Stretch" Value="Fill" />

        <Setter Property="RenderTransform">
            <Setter.Value>
                <RotateTransform />
            </Setter.Value>
        </Setter>

        <Setter Property="RenderOptions.BitmapScalingMode" Value="NearestNeighbor" />
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />

        <Setter Property="RenderTransformOrigin" Value="0.5 0.5" />

        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=Tag, RelativeSource={RelativeSource Mode=Self}}" Value="true">
                <DataTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation
                                RepeatBehavior="Forever"
                                Storyboard.TargetProperty="(Shape.RenderTransform).(RotateTransform.Angle)"
                                From="0"
                                To="360"
                                Duration="0:0:2" />
                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.EnterActions>
            </DataTrigger>
        </Style.Triggers>

    </Style>

    <Style x:Key="PulseUpImage" TargetType="{x:Type Image}">


        <Setter Property="Stretch" Value="Fill" />

        <Setter Property="RenderTransform">
            <Setter.Value>
                <ScaleTransform />
            </Setter.Value>
        </Setter>

        <Setter Property="RenderOptions.BitmapScalingMode" Value="Fant" />
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />

        <Setter Property="RenderTransformOrigin" Value="0.5 0.5" />

        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=Tag, RelativeSource={RelativeSource Mode=Self}}" Value="true">
                <DataTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetProperty="(Image.RenderTransform).(ScaleTransform.ScaleX)"
                                From="1.0"
                                To="1.2"
                                Duration="0:0:0.5" />
                            <DoubleAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetProperty="(Image.RenderTransform).(ScaleTransform.ScaleY)"
                                From="1.0"
                                To="1.2"
                                Duration="0:0:0.5" />

                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.EnterActions>
            </DataTrigger>
        </Style.Triggers>

    </Style>

    <Style x:Key="PulseDownImage" TargetType="{x:Type Image}">


        <Setter Property="Stretch" Value="Fill" />

        <Setter Property="RenderTransform">
